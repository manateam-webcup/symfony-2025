    <!-- Start Blog Area -->
    <div class="blog-area ptb-125">
      <div class="container">
        <ul
          class="main-blog-buttons text-center ps-0 list-unstyled"
          data-cue="slideInUp"
        >
          <li class="d-inline-block">
            <a href="blog.html" class="d-block active"> All endings </a>
          </li>
          <li class="d-inline-block">
            <a href="{{ path('app_ending_index', {'emotion': 'happy'}) }}" class="d-block class="btn btn-sm {% if emotion == 'sad' %} active {% else %}btn-outline-info{% endif %}""> Happy </a>
          </li>
          <li class="d-inline-block">
            <a href="{{ path('app_ending_index', {'emotion': 'sad'}) }}" class="d-block"> Sad </a>
          </li>
          <li class="d-inline-block">
            <a href="{{ path('app_ending_index', {'emotion': 'angry'}) }}" class="d-block"> Angry </a>
          </li>
          <li class="d-inline-block">
            <a href="{{ path('app_ending_index', {'emotion': 'shocking'}) }}" class="d-block"> Shocked </a>
          </li>
          <li class="d-inline-block">
            <a href="{{ path('app_ending_index', {'emotion': 'frustrated'}) }}" class="d-block"> Frustrated </a>
          </li>
          <li class="d-inline-block">
            <a href="{{ path('app_ending_index', {'emotion': 'depressed'}) }}" class="d-block"> Depression </a>
          </li>
        </ul>
        <div class="main-blogs-list" data-cues="slideInUp">
          <div class="item">
            {% if endings|length > 0 %}
              <div class="row align-items-center">
                {% set count = 1 %}
                {% for ending in endings %}
                  {% set count = count + 1 %}
                  {% if count is even%}
                    <div class="col-lg-6 col-md-6">
                      <div class="image">
                        <a href="{{ path('app_ending_show', {'id': ending.id}) }}" class="embed-responsive embed-responsive-4by3">
                          <img
                                  src="{{ asset('uploads/' ~ ending.image1Path) }}"
                                  alt="blog-image" class="img-fluid"
                          />
                        </a>
                        <div
                                class="info d-flex align-items-center justify-content-between"
                        >
                          <div class="date d-flex align-items-center">
                            <a href="{{ path('app_ending_show', {'id': ending.id}) }}" class="comments d-block">
                              <p>{{ ending.comments|length }} comments</p>
                            </a>
                            <span class="d-block"> {{ ending.createdAt|date("m/d/Y") }} </span>
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="col-lg-6 col-md-6">
                      <div class="content">
                        <h2 class="fw-normal">
                          <a href="{{ path('app_ending_show', {'id': ending.id}) }}">
                            {{ ending.title }}
                          </a>
                        </h2>
                        <a
                                href="{{ path('app_ending_show', {'id': ending.id}) }}"
                                class="link-btn d-flex align-items-center"
                        >
                          <i class="ri-arrow-right-up-line"></i>
                          <span class="d-inline-block"> Read More </span>
                        </a>
                      </div>
                    </div>
                  {% else %}
                    <div class="col-lg-6 col-md-6">
                      <div class="content">
                        <h2 class="fw-normal">
                          <a href="{{ path('app_ending_show', {'id': ending.id}) }}">
                            {{ ending.title }}
                          </a>
                        </h2>
                        <a
                                href="{{ path('app_ending_show', {'id': ending.id}) }}"
                                class="link-btn d-flex align-items-center"
                        >
                          <i class="ri-arrow-right-up-line"></i>
                          <span class="d-inline-block"> Read More </span>
                        </a>
                      </div>
                    </div>
                    <div class="col-lg-6 col-md-6">
                      <div class="image">
                        <a href="{{ path('app_ending_show', {'id': ending.id}) }}" class="d-block">
                          <img
                                  src="{{ asset('uploads/' ~ ending.image1Path) }}"
                                  alt="blog-image"
                          />
                        </a>
                        <div
                                class="info d-flex align-items-center justify-content-between"
                        >
                          <div class="date d-flex align-items-center">
                            <a href="{{ path('app_ending_show', {'id': ending.id}) }}" class="comments d-block">
                              <p>{{ ending.comments|length }} comments</p>
                            </a>
                            <span class="d-block"> {{ ending.createdAt|date("m/d/Y") }} </span>
                          </div>
                        </div>
                      </div>
                    </div>
                  {% endif %}

                {% endfor %}
              </div>
            {% else %}
              <div class="alert alert-info">
                <p class="mb-0">No endings found. Be the first to share your story!</p>
              </div>
            {% endif %}
          </div>

        </div>
